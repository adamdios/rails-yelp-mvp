<h1>Restaurants#new</h1>
<div class="container">
  <div class="row">
    <%= simple_form_for [@restaurant] do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>

    <%= f.submit "Add a restaurant", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
